{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "includeFiles": ["rice_model.keras"],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  }
}
